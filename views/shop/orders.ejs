<body>
    <%- include('../includes/navigation.ejs') %>
    <main class="centered">
        <% if (orders.length <= 0) { %>
            <h1>Nothihng there</h1>
        <%} else { %>
            <ul>
                <% orders.forEach(order => { %>
                    <li>
                        <h1># <%= order.id %></h1>
                        <ul>
                            <% order.products.forEach(item => { %>
                                <li> <%= item.title %> (<%= item.orderItem.quantity %>)</li>
                                <% }); %>
                        </ul>
                    </li>
                <%});%>
            </ul>
             <%} %>
    </main>
    <%- include('../includes/end.ejs') %>
